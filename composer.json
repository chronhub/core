{
  "name": "chronhub/core",
  "description": "core",
  "keywords": [
    "core"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "steph bug",
      "email": "chronhubgit@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "9.x"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "nunomaduro/larastan": "^2.1",
    "jangregor/phpstan-prophecy": "dev-master",
    "phpspec/prophecy-phpunit": "dev-master",
    "laravel/pint": "^0.2",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Chronhub\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chronhub\\Core\\Tests\\": "tests"
    }
  },
  "replace": {
    "chronhub/foo": "self.version",
    "chronhub/bar": "self.version"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/chronhub/foo"
    },
    {
      "type": "vcs",
      "url": "https://github.com/chronhub/bar"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "pint": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}