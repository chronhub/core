{
  "name": "chronhub/core",
  "description": "core",
  "keywords": [
    "stream"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "steph bug",
      "email": "chronhubgit@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "illuminate/contracts": "9.x",
    "laravel/pint": "^0.2",
    "nunomaduro/larastan": "^2.1",
    "jangregor/phpstan-prophecy": "dev-master",
    "phpspec/prophecy-phpunit": "dev-master",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Chronhub\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chronhub\\Core\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "pint": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}